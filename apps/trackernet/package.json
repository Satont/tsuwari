{
  "name": "@tsuwari/trackernet",
  "type": "module",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "dev": "reflex -s -r '\\.ts$' -- sh -c 'cross-env TS_NODE_TRANSPILE_ONLY=true node --loader ts-node/esm --enable-source-maps --trace-warnings --inspect=0.0.0.0:9233 --nolazy src/index.ts'",
    "start": "node --enable-source-maps dist/index.js"
  },
  "dependencies": {
    "@tsuwari/config": "workspace:*",
    "@tsuwari/grpc": "workspace:*",
    "@tsuwari/shared": "workspace:*",
    "@tsuwari/typeorm": "workspace:*",
    "@types/selenium-webdriver": "^4.1.15",
    "ioredis": "5.2.4",
    "nice-grpc": "^2.1.4",
    "selenium-webdriver": "^4.10.0"
  }
}
