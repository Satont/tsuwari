{
  "github": {
    "silent": true
  },
  "buildCommand": "pnpm turbo run build --filter=web",
  "builds": [
    {
      "src": "dist/server/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server/index.js"
    }
  ]
}