{
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "./dist/server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.tsuwari.app/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://api.tsuwari.app/:path*/"
    },
    {
      "source": "/dashboard/:path*",
      "destination": "https://dashboard.tsuwari.app/:path*"
    },
    {
      "source": "/dashboard/:path*/",
      "destination": "https://dashboard.tsuwari.app/:path*/"
    }
  ]
}
