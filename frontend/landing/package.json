{
  "name": "@tsuwari/landing",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm build:app && pnpm build:server",
    "build:app": "vite build",
    "vercel-build": "pnpm turbo run build --filter=web",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "node ./dist/server/index.js",
    "serve:dev": "cross-env NODE_ENV=development PORT=3005 tsx ./src/server/index.ts",
    "serve:prod": "cross-env NODE_ENV=production PORT=3005 node ./dist/server/index.js",
    "serve:prod:proxy": "cross-env NODE_ENV=production PORT=3005 API_PROXY=http://localhost:3002 node ./dist/server/index.js"
  },
  "devDependencies": {
    "@tsuwari/shared": "workspace:*",
    "@tsuwari/ui-components": "workspace:*",
    "@tsuwari/ui-fonts": "workspace:*",
    "@tsuwari/ui-theme": "workspace:*",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "tailwindcss-animation-delay": "^1.0.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-svg-loader": "^4.0.0",
    "vue-router": "^4.1.6"
  },
  "dependencies": {
    "@egjs/flicking-plugins": "^4.7.0",
    "@egjs/vue3-flicking": "^4.10.6",
    "@fastify/compress": "^6.2.0",
    "@fastify/http-proxy": "^8.4.0",
    "@fastify/middie": "^8.1.0",
    "@fastify/static": "^6.9.0",
    "@nanostores/persistent": "^0.7.0",
    "@nanostores/vue": "^0.7.0",
    "@tanstack/vue-query": "^4.24.10",
    "@vue/server-renderer": "^3.2.47",
    "@vueuse/core": "^9.13.0",
    "cross-env": "^7.0.3",
    "fastify": "^4.13.0",
    "nanostores": "^0.7.4",
    "vite-plugin-ssr": "^0.4.87",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2"
  }
}
