{
  "name": "@tsuwari/grpc",
  "private": true,
  "type": "module",
  "module": "./dist/src/index.js",
  "main": "./dist/src/index.js",
  "scripts": {
    "prebuild": "rm -rf .dist",
    "build": "pnpm gen && nest build",
    "gen": "tsproto --path ./src --template template.hbs",
    "go": "protoc --proto_path=. --go_out=./go --go-grpc_out=./go ./src/*.proto",
    "dev": "nodemon"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.6",
    "@nestjs/schematics": "^8.0.11",
    "nestjs-proto-gen-ts": "^1.0.20",
    "nodemon": "^2.0.16"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@nestjs/microservices": "^8.4.5",
    "rxjs": "^7.5.5"
  }
}