{
  "name": "@tsuwari/grpc",
  "scripts": {
    "build": "rm -rf dist && ts-node --transpileOnly gen.ts && tsc --module ESNext --outDir dist/esm --declaration false --declarationMap false && tsc --module CommonJS --declaration false --declarationMap false --outDir dist/cjs && tsc --emitDeclarationOnly --outDir dist/types"
  },
  "exports": {
    "./*": {
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/*.d.ts"
      ]
    }
  },
  "dependencies": {
    "@protobuf-ts/plugin": "^2.9.0",
    "@protobuf-ts/runtime": "^2.9.0",
    "@protobuf-ts/runtime-rpc": "^2.9.0",
    "@protobuf-ts/twirp-transport": "^2.9.0",
    "grpc-tools": "^1.11.3",
    "long": "^5.2.1",
    "nice-grpc": "^2.0.1",
    "nice-grpc-common": "^2.0.0",
    "protobufjs": "7.1.2",
    "ts-proto": "^1.135.0"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.8.3"
  }
}
