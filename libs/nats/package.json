{
  "name": "@tsuwari/nats",
  "type": "module",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "ts-node --esm --transpileOnly gen.ts",
    "ts:build": "tsc"
  },
  "exports": {
    "./parser": "./dist/parser/parser.js",
    "./bots": "./dist/bots/bots.js",
    "./dota": "./dist/dota/dota.js",
    "./eval": "./dist/eval/eval.js",
    "./timers": "./dist/timers/timers.js",
    "./integrations": "./dist/integrations/integrations.js"
  },
  "typesVersions": {
    "*": {
      "parser": [
        "dist/parser/parser.d.ts"
      ],
      "bots": [
        "dist/bots/bots.d.ts"
      ],
      "dota": [
        "dist/dota/dota.d.ts"
      ],
      "eval": [
        "dist/eval/eval.d.ts"
      ],
      "timers": [
        "dist/timers/timers.d.ts"
      ],
      "integrations": [
        "dist/integrations/integrations.d.ts"
      ]
    }
  },
  "dependencies": {
    "@protobuf-ts/runtime": "^2.8.0"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.8.0"
  }
}
