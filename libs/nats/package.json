{
  "name": "@tsuwari/nats",
  "type": "module",
  "scripts": {
    "gen:go": "protoc --proto_path=./protos --go_out=./parser/ --go_opt=paths=source_relative parser.proto",
    "gen:ts": "protoc --plugin=./node_modules/.bin/protoc-gen-ts --ts_out ./parser/ --ts_opt long_type_string --proto_path ./protos  --ts_opt generate_dependencies parser.proto",
    "gen": "pnpm gen:go && pnpm gen:ts"
  },
  "exports": {
    "./parser": "./dist/parser.js"
  },
  "dependencies": {
    "@protobuf-ts/runtime": "^2.8.0"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.8.0"
  }
}
