{
  "name": "@tsuwari/nats",
  "type": "module",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "ts-node --esm --transpileOnly gen.ts",
    "ts:build": "tsc"
  },
  "exports": {
    "./parser": "./dist/parser/parser.js",
    "./bots": "./dist/bots/bots.js",
    "./dota": "./dist/dota/dota.js"
  },
  "typesVersions": {
    "*": {
      "parser": [
        "dist/parser/parser.d.ts"
      ],
      "bots": [
        "dist/bots/bots.d.ts"
      ],
      "dota": [
        "dist/dota/dota.d.ts"
      ]
    }
  },
  "dependencies": {
    "@protobuf-ts/runtime": "^2.8.0"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.8.0"
  }
}
