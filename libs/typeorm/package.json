{
  "name": "@twir/typeorm",
  "type": "commonjs",
  "exports": {
    ".": "./dist/index.js",
    "./entities/*": "./dist/entities/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "cli": "typeorm-ts-node-commonjs -d ./src/index.ts",
    "deploy": "pnpm ts-node --transpileOnly ./runMigrations.ts",
    "migration:generate": "pnpm ts-node --transpileOnly ./generateMigration.ts",
    "setup": "pnpm run deploy && pnpm run seed",
    "seed": "pnpm ts-node --transpileOnly ./seed.ts"
  },
  "dependencies": {
    "@twir/config": "workspace:*",
    "@twir/crypto": "workspace:*",
    "pg": "8.8.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "10.9.1",
    "typeorm": "^0.3.17",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "dotenv": "^16.0.3"
  }
}
